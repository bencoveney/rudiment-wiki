{
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "concurrently": "^8.2.2",
    "http-server": "^14.1.1",
    "nodemon": "^3.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.2"
  },
  "type": "module",
  "scripts": {
    "build": "ts-node-esm ./src/index.tsx",
    "dev": "concurrently --names ts-node,server \"nodemon --watch src -e ts,tsx --exec 'ts-node-esm' ./src/index.tsx\" \"http-server ./build\""
  }
}
